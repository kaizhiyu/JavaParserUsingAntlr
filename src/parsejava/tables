create table JAVA_FILE_ANALYSIS(
    RECORD_NUM              INT             NOT NULL,
    PACKAGE_NAME            VARCHAR(50)     NOT NULL,
    CLASS_NAME              VARCHAR(30)     NOT NULL,
    KEYWORDS_TOTAL          INT,
    KEYWORDS_UNIQUE         INT,
    INDENTIFIERS_TOTAL      INT,
    IDENTIFIERS_UNIQUE      INT,
    CONSTANTS_TOTAL         INT,
    CONSTANTS_UNIQUE        INT,
    SPECIAL_CHARS_TOTAL     INT,
    SPECIAL_CHARS_UNIQUE    INT,
    TOTAL_CHAR_IN_FILE      INT,
    WHITE_SPACES            INT,
    COMMENT_CHARS           INT,
    WHITE_SPACES_PER        INT,
    COMMENT_CHARS_PER       INT,
    CREATE_TIME             TIMESTAMP,
    PRIMARY KEY (RECORD_NUM)
);

create table FILE_METHOD_COMPLEXITY(
    RECORD_NUM             INT             NOT NULL,
    METHOD_NAME            VARCHAR(50)     NOT NULL,
    CYCLOMATIC_COMPLEXITY  INT             NOT NULL,
    FOREIGN KEY (RECORD_NUM)
    REFERENCES JAVA_FILE_ANALYSIS(RECORD_NUM)
);